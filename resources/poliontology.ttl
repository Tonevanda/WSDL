#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/duty
:duty rdf:type owl:ObjectProperty ;
      rdfs:domain :LegislatureContext ;
      rdfs:range :Duty .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/dutyType
:dutyType rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Duty ;
          rdfs:range skos:Concept .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/electoralCircle
:electoralCircle rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :LegislatureContext ;
                 rdfs:range :ElectoralCircle .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/group
:group rdf:type owl:ObjectProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Membership ;
       rdfs:range :ParliamentaryGroup .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/habilitation
:habilitation rdf:type owl:ObjectProperty ;
              rdfs:domain :MoP ;
              rdfs:range :Habilitation .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasAcademicTitle
:hasAcademicTitle rdf:type owl:ObjectProperty ;
                  rdfs:domain :MoP ;
                  rdfs:range :AcademicTitle .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :MoP ;
         rdfs:range :Role .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/legislature
:legislature rdf:type owl:ObjectProperty ;
             owl:inverseOf :session ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :LegislativeSession ,
                         :LegislatureContext ;
             rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/legislatures
:legislatures rdf:type owl:ObjectProperty ;
              rdfs:domain :ElectoralCircle ;
              rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/membership
:membership rdf:type owl:ObjectProperty ;
            rdfs:domain :LegislatureContext ;
            rdfs:range :Membership .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/servedDuring
:servedDuring rdf:type owl:ObjectProperty ;
              rdfs:domain :MoP ;
              rdfs:range :LegislatureContext .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/session
:session rdf:type owl:ObjectProperty ;
         rdfs:domain :Legislature ;
         rdfs:range :LegislativeSession .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/situation
:situation rdf:type owl:ObjectProperty ;
           rdfs:domain :LegislatureContext ;
           rdfs:range :Situation .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/situationType
:situationType rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Situation ;
               rdfs:range skos:Concept .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/parliamentaryName
:parliamentaryName rdf:type owl:DatatypeProperty ;
                   rdfs:domain :LegislatureContext ;
                   rdfs:range xsd:string .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/tipoId
:tipoId rdf:type owl:DatatypeProperty ;
        rdfs:domain :Habilitation ;
        rdfs:range xsd:int .


