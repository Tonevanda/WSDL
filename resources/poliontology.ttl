#################################################################

###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/duringLegislature
:duringLegislature rdf:type owl:ObjectProperty ;
                   owl:inverseOf :hasSession ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :LegislativeSession ;
                   rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/dutyType
:dutyType rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Duty ;
          rdfs:range skos:Concept .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/electedIn
:electedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :MoP ;
           rdfs:range :ElectoralCircle .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasDuty
:hasDuty rdf:type owl:ObjectProperty ;
         rdfs:domain :MoP ;
         rdfs:range :Duty .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasInterestRegistry
:hasInterestRegistry rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :MoP ;
                     rdfs:range :InterestRegistry .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasMembership
:hasMembership rdf:type owl:ObjectProperty ;
               rdfs:domain :MoP ;
               rdfs:range :Membership .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/hasSession
:hasSession rdf:type owl:ObjectProperty ;
            rdfs:domain :Legislature ;
            rdfs:range :LegislativeSession .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/inLegislature
:inLegislature rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :ElectoralCircle ;
               rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/inSituation
:inSituation rdf:type owl:ObjectProperty ;
             rdfs:domain :MoP ;
             rdfs:range :Situation .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/memberDuringLegislature
:memberDuringLegislature rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain :Membership ;
                         rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/membershipOfGroup
:membershipOfGroup rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Membership ;
                   rdfs:range :ParliamentaryGroup .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/servedBy
:servedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :servesDuring ;
          rdfs:domain :Legislature ;
          rdfs:range :MoP .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/servesDuring
:servesDuring rdf:type owl:ObjectProperty ;
              rdfs:domain :MoP ;
              rdfs:range :Legislature .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/situationType
:situationType rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Situation ;
               rdfs:range skos:Concept .


:habilitation rdf:type owl:ObjectProperty ;
               rdfs:domain :MoP ;
               rdfs:range :Habilitation .

:hasRole rdf:type owl:ObjectProperty ;
               rdfs:domain :MoP ;
               rdfs:range :Role .

:hasAcademicTitle rdf:type owl:ObjectProperty ;
               rdfs:domain :MoP ;
               rdfs:range :Academic_Title .

#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/bid
:bid rdf:type owl:DatatypeProperty ,
              owl:FunctionalProperty ;
     rdfs:domain :MoP ;
     rdfs:range xsd:int .


###  http://www.semanticweb.org/tiago/ontologies/2025/11/poliontology/parliamentaryName
:parliamentaryName rdf:type owl:DatatypeProperty ;
                   rdfs:domain :MoP ;
                   rdfs:range xsd:string .

:tipoId rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Habilitation ;
                   rdfs:range xsd:int .

