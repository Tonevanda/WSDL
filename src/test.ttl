:electedIn a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :ElectoralCircle .

:hasDuty a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :MoPDuty .

:hasInterestRegistry a owl:FunctionalProperty,
        owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :InterestRegistry .

:hasLegislativeSession a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :Legislature ;
    rdfs:range :LegislativeSession .

:inLegislature a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :ElectoralCircle ;
    rdfs:range :Legislature .

:inSituation a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :MoPSituation .

:isMemberOf a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :ParliamentaryGroup .

:parliamentaryName a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :MoP ;
    rdfs:range xsd:string .

:representedInLegislature a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :ParliamentaryGroup ;
    rdfs:range :Legislature .

:servesDuring a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain :MoP ;
    rdfs:range :Legislature .

rdfs:label a owl:DatatypeProperty ;
    rdfs:domain :ElectoralCircle,
        :MoPDuty,
        :MoPSituation .

owl:topDataProperty rdfs:subPropertyOf :bid .

skos:altLabel a owl:DatatypeProperty ;
    rdfs:domain :ParliamentaryGroup .

skos:prefLabel a owl:DatatypeProperty ;
    rdfs:domain :ParliamentaryGroup .

foaf:accountName a owl:DatatypeProperty ;
    rdfs:domain :MoP .

schema:endDate a owl:DatatypeProperty ;
    rdfs:domain :LegislativeSession,
        :Legislature,
        :MoPDuty,
        :MoPSituation .

schema:position a owl:DatatypeProperty ;
    rdfs:domain :LegislativeSession,
        :Legislature .

schema:startDate a owl:DatatypeProperty ;
    rdfs:domain :LegislativeSession,
        :Legislature,
        :MoPDuty,
        :MoPSituation .

:InterestRegistry a owl:Class .

:bid a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :MoP ;
    rdfs:range xsd:int .

ocd:Deputato a owl:Class .

ocd:Legislatura a owl:Class .

ocd:gruppoParlamentare a owl:Class .

m4sc:Parlamentarian a owl:Class .

schema:Person a owl:Class .

:ElectoralCircle a owl:Class .

:LegislativeSession a owl:Class .

:MoPSituation a owl:Class .

:MoPDuty a owl:Class .

:Legislature a owl:Class ;
    owl:equivalentClass ocd:Legislatura .

:MoP a owl:Class ;
    rdfs:subClassOf schema:Person ;
    owl:equivalentClass ocd:Deputato,
        m4sc:Parlamentarian .

:XVII a :Legislature ;
    rdfs:label "XVII Legislature"@en,
        "XVII Legislatura"@pt ;
    schema:endDate "None"^^xsd:date ;
    schema:position "17"^^xsd:int ;
    schema:startDate "2025-06-03T00:00:00"^^xsd:date .

:ParliamentaryGroup a owl:Class ;
    owl:equivalentClass ocd:gruppoParlamentare .

